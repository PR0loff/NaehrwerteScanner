<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.testapplication.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_savedgroceries"
        android:name="com.example.testapplication.ui.home.SavedGroceriesFragment"
        android:label="Saved Groceries"
        tools:layout="@layout/fragment_savedgroceries" />

    <fragment
        android:id="@+id/navigation_recipe"
        android:name="com.example.testapplication.ui.notifications.RecipeFragment"
        android:label="Recipes"
        tools:layout="@layout/fragment_recipe" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.testapplication.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />

</navigation>